{
	"name": "buffetbotjs",
	"version": "1.0.0",
	"private": true,
	"description": "Bot I created for my private Discord",
	"author": {
		"email": "packageJson@danielTurcich.com",
		"name": "Daniel Turcich",
		"url": "https://danielTurcich.com"
	},
	"engines": {
		"node": "^22.2.0",
		"pnpm": "^9.1.4"
	},
	"type": "module",
	"scripts": {
		"serve": "pnpm compile && pnpm execute",
		"compile": "parcel build",
		"execute": "node --experimental-json-modules ./dist/index.js",
		"checks": "pnpm lint-check && pnpm format-check && pnpm type-check",
		"lint-fix": "eslint --fix \"**/*.{ts,vue}\"",
		"lint-check": "eslint \"**/*.{ts,vue}\"",
		"format-fix": "prettier --check --write \"**/**\" ",
		"format-check": "prettier --check \"**/**\"",
		"type-check": "tsc --noEmit",
		"prepare": "husky"
	},
	"dependencies": {
		"@discordjs/opus": "^0.9.0",
		"@discordjs/voice": "^0.17.0",
		"discord.js": "^14.16.2",
		"libsodium-wrappers": "^0.7.15",
		"lodash.isequal": "^4.5.0"
	},
	"devDependencies": {
		"@types/lodash.isequal": "^4.5.8",
		"@types/node": "^22.5.5",
		"@types/ws": "^8.5.12",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"husky": "^9.1.6",
		"parcel": "^2.12.0",
		"prettier": "^3.3.3",
		"typescript": "^5.6.2",
		"undici": "^6.19.8"
	},
	"source": "./src/main.ts",
	"module": "./dist/index.js",
	"targets": {
		"module": {
			"context": "node",
			"isLibrary": false,
			"sourceMap": false
		}
	}
}
