{
	"name": "buffetbotjs",
	"version": "1.0.0",
	"private": true,
	"description": "Bot I created for my private Discord",
	"author": {
		"email": "packageJson@danielTurcich.com",
		"name": "Daniel Turcich",
		"url": "https://danielTurcich.com"
	},
	"engines": {
		"node": "^21.0.0",
		"pnpm": "^8.15.0"
	},
	"type": "module",
	"scripts": {
		"serve": "pnpm compile && pnpm execute",
		"compile": "parcel build",
		"execute": "node --experimental-json-modules ./dist/index.js",
		"checks": "pnpm lint-check && pnpm format-check && pnpm type-check",
		"lint-fix": "eslint --fix \"**/*.{ts,vue}\"",
		"lint-check": "eslint \"**/*.{ts,vue}\"",
		"format-fix": "prettier --check --write \"**/**\" ",
		"format-check": "prettier --check \"**/**\"",
		"type-check": "tsc --noEmit",
		"prepare": "husky"
	},
	"dependencies": {
		"@discordjs/opus": "^0.9.0",
		"@discordjs/voice": "^0.16.1",
		"discord.js": "^14.14.1",
		"libsodium-wrappers": "^0.7.13",
		"lodash.isequal": "^4.5.0"
	},
	"devDependencies": {
		"@types/lodash.isequal": "^4.5.8",
		"@types/node": "^20.12.6",
		"@types/ws": "^8.5.10",
		"@typescript-eslint/eslint-plugin": "^7.6.0",
		"@typescript-eslint/parser": "^7.6.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"husky": "^9.0.11",
		"parcel": "^2.12.0",
		"prettier": "^3.2.5",
		"typescript": "^5.4.4",
		"undici": "^6.12.0"
	},
	"source": "./src/main.ts",
	"module": "./dist/index.js",
	"targets": {
		"module": {
			"context": "node",
			"isLibrary": false,
			"sourceMap": false
		}
	}
}
