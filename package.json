{
	"name": "buffetbotjs",
	"version": "1.0.0",
	"private": false,
	"main": "./dist/src/buffetBotMain.js",
	"description": "Bot I created for my private Discord",
	"author": {
		"email": "packageJson@danielTurcich.com",
		"name": "Daniel Turcich",
		"url": "https://danielTurcich.com"
	},
	"engines": {
		"node": "^18.0.0",
		"pnpm": "^7.0.0"
	},
	"type": "module",
	"scripts": {
		"serve": "pnpm compile && pnpm execute",
		"compile": "pnpm exec tsc",
		"execute": "node --experimental-json-modules ./dist/buffetBotMain.js",
		"lint-fix": "eslint --fix \"**/*.{ts,vue}\"",
		"lint-check": "eslint \"**/*.{ts,vue}\"",
		"format-fix": "prettier --check --write \"**/**\" ",
		"format-check": "prettier --check \"**/**\"",
		"type-check": "tsc --noEmit",
		"prepare": "husky install"
	},
	"dependencies": {
		"discord.js": "^14.3.0",
		"file-system": "^2.2.2",
		"lodash.isequal": "^4.5.0",
		"node-fetch": "^3.2.10"
	},
	"devDependencies": {
		"@types/lodash.isequal": "^4.5.6",
		"@types/node": "^18.7.11",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.34.0",
		"@typescript-eslint/parser": "^5.34.0",
		"eslint": "^8.22.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"prettier": "^2.7.1",
		"typescript": "^4.7.4"
	}
}
