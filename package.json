{
	"name": "buffetbotjs",
	"version": "1.0.0",
	"private": true,
	"description": "Bot I created for my private Discord",
	"author": {
		"email": "packageJson@danielTurcich.com",
		"name": "Daniel Turcich",
		"url": "https://danielTurcich.com"
	},
	"engines": {
		"node": "^19.0.0",
		"pnpm": "^8.1.0"
	},
	"type": "module",
	"scripts": {
		"serve": "pnpm compile && pnpm execute",
		"compile": "parcel build",
		"execute": "node --experimental-json-modules ./dist/index.js",
		"checks": "pnpm lint-check && pnpm format-check && pnpm type-check",
		"lint-fix": "eslint --fix \"**/*.{ts,vue}\"",
		"lint-check": "eslint \"**/*.{ts,vue}\"",
		"format-fix": "prettier --check --write \"**/**\" ",
		"format-check": "prettier --check \"**/**\"",
		"type-check": "tsc --noEmit",
		"prepare": "husky install"
	},
	"dependencies": {
		"discord.js": "14.7.1",
		"file-system": "^2.2.2",
		"lodash.isequal": "^4.5.0"
	},
	"devDependencies": {
		"@types/lodash.isequal": "^4.5.6",
		"@types/node": "^18.15.11",
		"@types/ws": "^8.5.4",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"parcel": "^2.8.3",
		"prettier": "^2.8.7",
		"typescript": "^5.0.3",
		"undici": "^5.21.0"
	},
	"source": "./src/main.ts",
	"module": "./dist/index.js",
	"targets": {
		"module": {
			"context": "node",
			"isLibrary": false,
			"sourceMap": false
		}
	}
}
